a.nav-expander {
  background: none repeat scroll 0 0;
  color: #F0F0F0;
  display: block;
  margin-left: 0;
  padding: 10px;
  position: absolute;
  left: auto;
  text-decoration: none;
  z-index: 3;
}
a.nav-expander:hover {
  cursor: pointer;
}
#nav {
  /*background: #424344;*/
  background: rgb(231,241,252);
  display: none;
  height: 100%;
  z-index:4;
  overflow: auto;
